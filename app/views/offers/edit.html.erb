
<div class="container">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">
    <%= simple_form_for @offer do |f| %>
      <%= f.input :company_name %>
      <%= f.input :address %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%#= f.input :photo %>
      <%#= f.input :photo_cache, as: :hidden %>
      <%= f.submit "OK", class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
</div>
